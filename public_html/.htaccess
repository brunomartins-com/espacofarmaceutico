#############################################################################################
Options +FollowSymLinks

# ATIVA AS URLS AMIGAVEIS
RewriteEngine On

RewriteBase /

#Verifica se a url digitada é um arquivo caso seja para aqui
RewriteCond %{REQUEST_FILENAME} !-f

#Verifica se a url digitada é uma pasta caso seja para aqui
RewriteCond %{REQUEST_FILENAME} !-d

## PAGINA INICIAL
RewriteRule ^/?$ index.php [NC,L]
RewriteRule ^erro-inativo/?$ index.php?erro=inativo [NC,L]
RewriteRule ^erro-incorreto/?$ index.php?erro=incorreto [NC,L]
RewriteRule ^confirma-envio/?$ index.php?erro=confirma-envio [NC,L]
RewriteRule ^erro-envio/?$ index.php?erro=envio [NC,L]
RewriteRule ^erro-inexistente/?$ index.php?erro=inexistente [NC,L]
RewriteRule ^erro-vazio/?$ index.php?erro=vazio [NC,L]

## BUSCA
RewriteRule ^busca/?$ busca.php [NC,L]

## O TEUTO
RewriteRule ^o-teuto/?$ teuto.php [NC,L]

## TEUTO 360
RewriteRule ^teuto-360/?$ teuto-360.php [NC,L]

## BLOG
RewriteRule ^blog/?$ blog.php [NC,L]
RewriteRule ^blog/pagina/([0-9]+)/?$ blog.php?pg=$1 [NC,L]
RewriteRule ^blog/busca/?$ blog.php [NC,L]
RewriteRule ^blog/busca/([a-z0-9-_]+)/?$ blog.php?$1 [NC,QSA]
RewriteRule ^blog/([0-9]{4})/([0-9]{2})/([0-9]{2})/([a-z0-9-]+)/?$ blog.php?leitura=sim&slug=$4&data=$1-$2-$3 [NC,L]

## EVENTOS
RewriteRule ^eventos/?$ eventos.php [NC,L]
RewriteRule ^eventos/busca/?$ eventos.php [NC,L]
RewriteRule ^eventos/pagina/([0-9]+)/?$ eventos.php?pg=$1 [NC,L]

## MEDICAMENTOS GENERICOS
RewriteRule ^medicamentos-genericos/?$ genericos-faq.php [NC,L]
RewriteRule ^medicamentos-genericos/perguntas-frequentes/?$ genericos-faq.php [NC,L]
RewriteRule ^medicamentos-genericos/conceito/?$ genericos-historia.php [NC,L]
RewriteRule ^medicamentos-genericos/confiabilidade-e-qualidade/?$ genericos-confiabilidade.php [NC,L]
RewriteRule ^medicamentos-genericos/legislacao/?$ genericos-legislacao.php [NC,L]
RewriteRule ^medicamentos-genericos/acoes-para-a-consolidacao-dos-genericos/?$ genericos-consolidacao.php [NC,L]
RewriteRule ^medicamentos-genericos/reducao-dos-gastos-com-a-saude/?$ genericos-gastos.php [NC,L]

## NOTICIAS E RELEASES
RewriteRule ^noticias-e-releases/?$ noticias.php [NC,L]
RewriteRule ^noticias-e-releases/pagina/([0-9]+)/?$ noticias.php?pg=$1 [NC,L]
RewriteRule ^noticias-e-releases/busca/?$ noticias.php [NC,L]
RewriteRule ^noticias-e-releases/busca/([a-z0-9-_]+)/?$ noticias.php?$1 [NC,QSA]
RewriteRule ^noticias-e-releases/([0-9]{4})/([0-9]{2})/([0-9]{2})/([a-z0-9-]+)/?$ noticias.php?leitura=sim&slug=$4&data=$1-$2-$3 [NC,L]

## ENTRETENIMENTO
RewriteRule ^entretenimento/?$ entretenimento.php [NC,L]
RewriteRule ^entretenimento/([a-z0-9-]+)/?$ entretenimento.php?leitura=sim&slug=$1 [NC,L]

## PRODUTOS
RewriteRule ^produtos/?$ produtos.php [NC,L]
RewriteRule ^produtos/pagina/([0-9]+)/?$ produtos.php?pg=$1 [NC,L]

RewriteRule ^produtos/categoria/([a-z0-9-]+)/?$ produtos.php?categoria=$1 [NC,L]
RewriteRule ^produtos/categoria/([a-z0-9-]+)/pagina/([0-9]+)/?$ produtos.php?categoria=$1&pg=$2 [NC,L]

RewriteRule ^produtos/busca/?$ produtos.php [NC,L]
RewriteRule ^produtos/categoria/([a-z0-9-]+)/busca/?$ produtos.php?categoria=$1 [NC,L]
RewriteRule ^produtos/busca/([a-z0-9-_]+)/?$ produtos.php?$1 [NC,QSA]
RewriteRule ^produtos/categoria/([a-z0-9-]+)/busca/([a-z0-9-_]+)/?$ produtos.php?$2&categoria=$1 [NC,QSA]

RewriteRule ^produtos/busca-nome/?$ produtos.php [NC,L]
RewriteRule ^produtos/busca-nome/([a-z0-9-]+)/pagina/([0-9]+)/?$ produtos.php?slug=$1&pg=$2 [NC,L]
RewriteRule ^produtos/categoria/([a-z0-9-]+)/busca-nome/?$ produtos.php?categoria=$1 [NC,L]
RewriteRule ^produtos/categoria/([a-z0-9-]+)/busca-nome/([a-z0-9-]+)/pagina/([0-9]+)/?$ produtos.php?categoria=$1&slug=$2&pg=$3 [NC,L]

RewriteRule ^produtos/busca-principio-ativo/?$ produtos.php [NC,L]
RewriteRule ^produtos/categoria/([a-z0-9-]+)/busca-principio-ativo/?$ produtos.php?categoria=$1 [NC,L]
RewriteRule ^produtos/busca-principio-ativo/([a-z0-9-_]+)/?$ produtos.php?$1 [NC,QSA]
RewriteRule ^produtos/categoria/([a-z0-9-]+)/busca-principio-ativo/([a-z0-9-_]+)/?$ produtos.php?$2&categoria=$1 [NC,QSA]

RewriteRule ^produtos/catalogos-digitais/?$ produtos_catalogos_digitais.php [NC,L]

## ARTIGOS
RewriteRule ^artigos/?$ artigos.php [NC,L]
RewriteRule ^artigos/pagina/([0-9]+)/?$ artigos.php?pg=$1 [NC,L]
RewriteRule ^artigos/busca/?$ artigos.php [NC,L]
RewriteRule ^artigos/busca/([a-z0-9-_]+)/?$ artigos.php?$1 [NC,QSA]
RewriteRule ^artigos/([0-9]{4})/([0-9]{2})/([0-9]{2})/([a-z0-9-]+)/?$ artigos.php?leitura=sim&slug=$4&data=$1-$2-$3 [NC,L]

## VIDEOS
RewriteRule ^videos/?$ videos.php?categoria=1 [NC,L]
RewriteRule ^campanhas/?$ videos.php?categoria=2 [NC,L]
RewriteRule ^videos-3d/?$ videos.php?categoria=3 [NC,L]

## TREINAMENTOS
RewriteRule ^universidade-teuto/?$ treinamentos.php [NC,L]

## MATERIAL DE APOIO
RewriteRule ^material-de-apoio/?$ material-apoio.php [NC,L]
RewriteRule ^material-de-apoio/pagina/([0-9]+)/?$ material-apoio.php?pg=$1 [NC,L]
RewriteRule ^material-de-apoio/google-body/?$ material-apoio-google-body.php [NC,L]
RewriteRule ^material-de-apoio/calculadora-imc/?$ material-apoio-imc.php [NC,L]
RewriteRule ^material-de-apoio/calculadora-imc/resultado/?$ material-apoio-imc.php?app=calculo [NC,L]

## CONTATO
RewriteRule ^contato/?$ contato.php [NC,L]
RewriteRule ^contato/envio/?$ contato.php?envio=sim [NC,L]

## CONSELHOS REGIONAIS
RewriteRule ^conselhos-regionais/?$ conselhos-regionais.php [NC,L]

## FARMACOVIGILANCIA
RewriteRule ^farmacovigilancia/?$ farmacovigilancia.php [NC,L]
RewriteRule ^farmacovigilancia/envio/?$ farmacovigilancia.php?envio=sim [NC,L]

## TRABALHE CONOSCO
RewriteRule ^trabalhe-conosco/?$ trabalhe-conosco.php [NC,L]

## CADASTRE-SE
RewriteRule ^cadastre-se/?$ cadastro.php [NC,L]
RewriteRule ^cadastre-se/envia/?$ cadastro.php?form=envia [NC,L]
RewriteRule ^cadastre-se/confirma/([a-z0-9-]+)/?$ cadastro.php?form=confirma&uid=$1 [NC,L]

## LOGAR
RewriteRule ^logar/?$ _php/LogarSite.php [NC,L]

## MEUS DADOS
RewriteRule ^meus-dados/?$ meus-dados.php [NC,L]
RewriteRule ^meus-dados/altera/?$ meus-dados.php?form=altera [NC,L]

#Pagina Painel Lembrar Senha
RewriteRule ^lembrar-senha/?$ _php/LembrarSenhaSite.php [NC,L]

#Pagina Recuperacao de Senha
RewriteRule ^recuperacao-de-senha/([a-z0-9-]+)/([a-z0-9-]+)/?$ recuperacao-de-senha.php?time=$1&uid=$2 [NC,L]
#Pagina Recuperacao de Senha Envio
RewriteRule ^nova-senha/envio/?$ recuperacao-de-senha.php?form=envio [NC,L]
#Pagina Cancela Recuperacao de Senha
RewriteRule ^cancela-recuperacao-de-senha/([a-z0-9-]+)/([a-z0-9-]+)/?$ recuperacao-de-senha.php?form=cancela&time=$1&uid=$2 [NC,L]

#Pagina Painel Sair
RewriteRule ^sair/?$ _php/Sair.php [NC,L]